<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <template id="theme_customize" inherit_id="theme_xtremo.theme_customize">
    <xpath expr="//content[@id='theme_xtremo_customize_color']/opt[last()]" position="after">
      <opt data-xmlid="theme_livep.livep_xtremo_color" data-icon="/theme_livep/static/img/logo_livep.png"/>
    </xpath>
    <xpath expr="//content[@id='theme_customize_page_header']/opt[last()]" position="after">
      <opt data-xmlid="theme_livep.livep_xtremo_header,theme_xtremo.xtremo_header_2_menu_content,theme_xtremo.xtremo_whishlist_button" data-reload="/*" data-icon="/theme_livep/static/img/logo_livep.png" />
    </xpath>
  </template>

</odoo>
