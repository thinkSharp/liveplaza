<odoo>
    <data>
        <record id="wk_otp_config" model="ir.ui.view">
            <field name="name">Addons Setting</field>
            <field name="model">webkul.website.addons</field>
            <field name="inherit_id" ref="website_webkul_addons.config_webkul_website_addons_form"/>
            <field name="arch" type="xml">
                <xpath expr="//table/tbody" position="after">
                     <tr>
                        <td>1</td>
                        <td><label for="module_otp_auth"/></td>
                        <td><field name="module_otp_auth"/></td>
                        <td>
                            <button type="action" name="otp_auth.action_website_otp_conf" string="Configuration" class="oe_link"></button>
                         </td>
                    </tr>
                </xpath>
            </field>
        </record>
    </data>
</odoo>